ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda/bin/nvcc

cflags = -DTORCH_EXTENSION_NAME=bias_act_plugin -DTORCH_API_INCLUDE_EXTENSION_H -isystem /root/miniconda3/envs/gan/lib/python3.12/site-packages/torch/include -isystem /root/miniconda3/envs/gan/lib/python3.12/site-packages/torch/include/torch/csrc/api/include -isystem /usr/local/cuda/include -isystem /root/miniconda3/envs/gan/include/python3.12 -fPIC -std=c++17
post_cflags = 
cuda_cflags = -DTORCH_EXTENSION_NAME=bias_act_plugin -DTORCH_API_INCLUDE_EXTENSION_H -isystem /root/miniconda3/envs/gan/lib/python3.12/site-packages/torch/include -isystem /root/miniconda3/envs/gan/lib/python3.12/site-packages/torch/include/torch/csrc/api/include -isystem /usr/local/cuda/include -isystem /root/miniconda3/envs/gan/include/python3.12 -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr -gencode=arch=compute_120,code=compute_120 -gencode=arch=compute_120,code=sm_120 --compiler-options '-fPIC' --use_fast_math --allow-unsupported-compiler -std=c++17
cuda_post_cflags = 
cuda_dlink_post_cflags = 
sycl_dlink_post_cflags = 
ldflags = -shared -L/root/miniconda3/envs/gan/lib/python3.12/site-packages/torch/lib -lc10 -lc10_cuda -ltorch_cpu -ltorch_cuda -ltorch -ltorch_python -L/usr/local/cuda/lib64 -lcudart

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc --generate-dependencies-with-compile --dependency-output $out.d $cuda_cflags -c $in -o $out $cuda_post_cflags





rule link
  command = $cxx $in $ldflags -o $out

build bias_act.o: compile /workspace/R3GAN/.cache/torch_extensions/bias_act_plugin/923aace9669174ee9624e9f9a8189528-nvidia-geforce-rtx-5090/bias_act.cpp
build bias_act.cuda.o: cuda_compile /workspace/R3GAN/.cache/torch_extensions/bias_act_plugin/923aace9669174ee9624e9f9a8189528-nvidia-geforce-rtx-5090/bias_act.cu





build bias_act_plugin.so: link bias_act.o bias_act.cuda.o

default bias_act_plugin.so
